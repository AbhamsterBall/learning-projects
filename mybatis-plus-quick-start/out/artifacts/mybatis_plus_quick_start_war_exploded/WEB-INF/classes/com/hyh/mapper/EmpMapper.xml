  <select id="selectMaxEmpNo" resultType="int">
    select max(empno) from emp
  </select>

  <insert id="inMultiple">
    insert all
    <foreach collection="records" item="emp" close="select * from dual">
      into emp values (#{emp.empEmpno}, #{emp.empEname}, #{emp.empJob}, #{emp.empMgr}, #{emp.empHiredate}, #{emp.empSal}, #{emp.empComm}, #{emp.empDeptno})
    </foreach>
  </insert>

